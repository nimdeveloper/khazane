{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    "sql:default",
    "store:default",
    "stronghold:default",
    "fs:write-all",
    "fs:write-files",
    "fs:allow-app-write-recursive",
    "fs:allow-appcache-write-recursive"
  ]
}
